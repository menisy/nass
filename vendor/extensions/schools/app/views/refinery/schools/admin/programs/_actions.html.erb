<ul>
  <% if ::Refinery::Schools::Admin::ProgramsController.searchable? %>
    <li>
      <%= render '/refinery/admin/search', :url => refinery.schools_admin_programs_path %>
    </li>
  <% end %>
  <li>
    <%= link_to t('.create_new'), refinery.new_schools_admin_program_path,
                 :class => "add_icon" %>
  </li>
<% if !searching? && ::Refinery::Schools::Admin::ProgramsController.sortable? && ::Refinery::Schools::Program.many? %>
  <li>
    <%= link_to t('.reorder', :what => "Programs"),
                 refinery.schools_admin_programs_path,
                 :id => "reorder_action",
                 :class => "reorder_icon" %>

    <%= link_to t('.reorder_done', :what => "Programs"),
                 refinery.schools_admin_programs_path,
                 :id => "reorder_action_done",
                 :style => "display: none;",
                 :class => "reorder_icon" %>
  </li>
<% end %>
</ul>
